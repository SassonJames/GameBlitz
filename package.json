{
  "name": "gameblitz",
  "version": "1.0.0",
  "description": "A fast paced mini game application where players compete for points",
  "main": "app.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "build": "babel ./client --out-file ./hosted/compiled.js",
    "watch": "babel --watch ./client --out-file ./hosted/compiled.js",
    "start": "node ./server/app.js",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"No Tests\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SassonJames/GameBlitz.git"
  },
  "dependencies": {
    "express": "^4.16.2",
    "node": "^9.2.0",
    "socket.io": "^2.0.4",
    "xxhashjs": "^0.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nodemon": "^1.11.0"
  },
  "author": "Jamie Sasson and Quang Dao",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/SassonJames/GameBlitz/issues"
  },
  "homepage": "https://github.com/SassonJames/GameBlitz#readme"
}
